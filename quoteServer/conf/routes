# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Application Controller
GET     /                           controllers.Application.index()
GET     /picture                    controllers.Application.uploadPicture()
GET     /requestImages              controllers.Application.requestImages(numRequested: int)


#User Controller
POST    /userUploadedImage          controllers.UserController.userUploadedImage()
POST    /newUser                    controllers.UserController.newUser(email: String, password: String)

# Quote Controller
# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)
